[node name="TPhrase6" parent="." instance=ExtResource("12_l5j3w")]
position = Vector2(1064, -136)
text = "How do you know it's not just a square that turns on and off like a 0/1 block?"

[node name="TPhrase7" parent="." instance=ExtResource("12_l5j3w")]
